  },
  "dependencies": {
    "normalize-email": "^1.1.1",
    "normalize-url": "^4.3.0"
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "coveralls": "^3.0.4",
    "husky": "2.7.0",
    "lint-staged": "^9.2.0",
    "nyc": "^14.1.1",
    "prettier-standard": "^9.1.1",
    "standard": "^13.0.1"
  }
