{
  "name": "normalize-email-or-url",
  "description": "Return normalized email or url from a string.",
  "version": "1.0.0",
  "main": "index.js",
  "license": "AGPL-3.0",
  "repository": "millette/normalize-email-or-url",
  "author": {
    "name": "Robin Millette",
    "email": "robin@millette.info",
    "url": "http://robin.millette.info"
  },
  "engines": {
    "node": ">= 10.16.0"
  },
  "keywords": [
    "normmalize",
    "email",
    "url"
  ],
  "husky": {
